<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_address"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.irinerose.app.myapplication.AddressActivity">

    <include layout="@layout/toolbar_nologo"
        android:id="@+id/include7" />

    <ListView
        android:id="@+id/address_list"
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="200dp"
        android:layout_below="@+id/include7"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/add_address" />

    <Button
        android:id="@+id/add_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Add Address"
        android:background="#00c23b"
        android:textColor="@color/white"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
